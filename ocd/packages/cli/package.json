{
  "name": "@ocd/cli",
  "version": "1.0.0",
  "description": "OKIT Open Cloud Designer Command-line",
  "title": "OKIT Open Cloud Designer DeskCommand-linetop",
  "author": {
    "name": "Andrew Hopkinson",
    "email": "andrew.hopkinson@oracle.com",
    "url": "https://github.com/oracle/oci-designer-toolkit"
  },
  "bugs": "https://github.com/oracle/oci-designer-toolkit/issues",
  "keywords": [
    "ocd",
    "okit"
  ],
  "license": "UPL-1.0",
  "main": "lib/esm/index.js",
  "module": "lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "mkdir -p dist && npm pack --pack-destination dist",
    "compile": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "build": "npm run compile && npm run pack",
    "generate-oci-reference-data": "node lib/esm/ocd-generate-default-cache.js  -d ../desktop/src/data",
    "test-export-to-svg": "node lib/esm/ocd-export-to-svg.js  -d ../../test/design.okit -o ../../test/ -c ../desktop/src/css/oci-theme.css -c ../desktop/src/css/ocd-svg.css",
    "test-export-to-md": "node lib/esm/ocd-export-to-md.js  -d ../../test/design.okit -o ../../test/okit.md -c ../desktop/src/css/oci-theme.css -c ../desktop/src/css/ocd-svg.css"
  },
  "dependencies": {
    "@ocd/codegen": "^1.0.0",
    "@ocd/core": "^1.0.0",
    "@ocd/export": "^1.0.0",
    "@ocd/import": "^1.0.0",
    "@ocd/model": "^1.0.0",
    "@ocd/query": "^1.0.0"
  }
}
